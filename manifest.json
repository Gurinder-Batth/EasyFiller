{
    "manifest_version": 2,
    "name": "Easy Filler",
    "description": "This extension will very helpful for developer to fill the dummy data into form",
    "version": "1.0",
    "content_scripts" : [
	{
			"matches" : [
            "<all_urls>"
                ],
            "js" : ["content.js"]
     }],
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "<all_urls>"
    ]
  }